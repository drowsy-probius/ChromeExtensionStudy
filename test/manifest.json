{
    "manifest_version": 2,
    "name": "testste",
    "version": "0.0.1",

    "description": "sttesst",

    "background": {
        "scripts": ["js/back/background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_popup": "popup.html"
    },

    "permissions": [
        "storage",
        "cookies",
        "webNavigation",
        "tabs",
        "notifications",
        "activeTab",
        "https://*.korea.ac.kr/*",
        "http://*.korea.ac.kr/*"
    ]
}